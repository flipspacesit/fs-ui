{
  "name": "@flipspacesit/fs-ui",
  "version": "1.0.3",
  "description": "Flipspaces shared UI component library",
  "type": "module",
  "main": "./dist/fs-ui.cjs.js",
  "module": "./dist/fs-ui.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/fs-ui.es.js",
      "require": "./dist/fs-ui.cjs.js"
    },
    "./styles": "./dist/style.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "prepublishOnly": "npm run build",
    "docs:dev": "cd docs && npm run dev",
    "docs:build": "cd docs && npm run build",
    "docs:install": "cd docs && npm install"
  },
  "peerDependencies": {
    "@emotion/react": "^11.0.0",
    "@emotion/styled": "^11.0.0",
    "@mui/material": "^5.0.0 || ^6.0.0 || ^7.0.0",
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0",
    "styled-components": "^6.0.0"
  },
  "devDependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@eslint/js": "^9.39.1",
    "@mui/material": "^7.3.7",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "@vitejs/plugin-react": "^5.1.1",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "notistack": "^3.0.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "styled-components": "^6.3.5",
    "typescript": "^5.2.2",
    "typescript-eslint": "^8.54.0",
    "vite": "^7.2.4",
    "vite-plugin-dts": "^4.5.0"
  },
  "dependencies": {
    "@mui/x-date-pickers": "^8.27.0",
    "dayjs": "^1.11.19",
    "notistack": "^3.0.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/flipspacesit/fs-ui.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "keywords": [
    "flipspaces",
    "ui",
    "react",
    "components",
    "mui"
  ],
  "author": "Flipspaces",
  "license": "MIT"
}
